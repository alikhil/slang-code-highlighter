{
    "name": "slang-code-highlighter",
    "displayName": "SLang",
    "description": "SLang code highlighter.",
    "version": "0.1.0",
    "publisher": "alikhil",
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "slang",
                "aliases": [
                    "SLang",
                    "slang"
                ],
                "extensions": [
                    ".slang"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "slang",
                "scopeName": "source.slang",
                "path": "./syntaxes/s.tmLanguage"
            }
        ]
    },
    "repository": {
        "type": "GitHub",
        "url": "https://github.com/alikhil/slang-code-highlighter.git"
    }
}